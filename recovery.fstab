# mount point   fstype          device

/sdcard         vfat            /dev/block/mmcblk0p1    /dev/block/mmcblk0
/system         ext3            /dev/block/system
/data           ext3            /dev/block/data
/cache          ext3            /dev/block/cache
/boot		mtd             /dev/block/boot
/systemorig     auto            /dev/block/systemorig

